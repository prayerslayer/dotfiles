{
  "editor.fontFamily": "Iosevka, Fira Code, Menlo, Monaco, monospace",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.control",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "comments": false,
    "other": false,
    "strings": true
  },
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.folding": false,
  "editor.tabCompletion": "on",
  "editor.cursorBlinking": "solid",
  "editor.fontLigatures": true,
  "editor.autoIndent": true,
  "editor.lineNumbers": "interval",
  "editor.rulers": [80, 120],
  "zenMode.centerLayout": false,
  "workbench.fontAliasing": "auto",
  "workbench.colorCustomizations": {
    "editorBracketMatch.background": "#FF1493",
    "editorBracketMatch.border": "#FF1493",
    "editorRuler.foreground": "#333"
  },
  "git.confirmSync": false,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontFamily": "Iosevka, Menlo, Monaco, monospace",
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/dist/**/*": true,
    "**/node_modules/**/*": true,
    "**/__pycache__/": true,
    "**/*.py.bak": true,
    ".vscode/": true,
    "**/.tox/**": true,
    "**/coverage/**": true
  },
  "search.exclude": {
    "**/node_modules": true
  },
  "trailing-spaces.trimOnSave": true,
  "trailing-spaces.includeEmptyLines": true,
  "window.zoomLevel": 0,
  "typescript.check.npmIsInstalled": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": false,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.hideCursorInOverviewRuler": true,
  "search.location": "sidebar",
  "workbench.tree.renderIndentGuides": "none",
  "editor.formatOnSave": true,
  "[python]": {
    "editor.tabSize": 4
  },
  "python.formatting.provider": "black",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontWeight": "400",
  "editor.minimap.enabled": false,
  "latex-workshop.message.update.show": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": true,
  "workbench.colorTheme": "Default High Contrast",
  "editor.suggest.filterGraceful": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "eslint.enable": true,
  "eslint.provideLintTask": true,
  "eslint.options": {
      "configFile": "./frontend/config/.eslintrc.yml"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
      { "language": "javascript", "autoFix": true },
      { "language": "javascriptreact", "autoFix": true },
      { "language": "typescript", "autoFix": true },
      { "language": "typescriptreact", "autoFix": true }
  ],
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ]
}
