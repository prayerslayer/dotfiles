{
  "editor.fontFamily": "Iosevka, Fira Code, Menlo, Monaco, monospace",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.control",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "comments": false,
    "other": false,
    "strings": true
  },
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.folding": false,
  "editor.tabCompletion": "on",
  "editor.cursorBlinking": "solid",
  "editor.fontLigatures": true,
  "editor.autoIndent": true,
  "editor.lineNumbers": "interval",
  "editor.rulers": [80, 120],
  "zenMode.centerLayout": false,
  "workbench.fontAliasing": "auto",
  "workbench.colorCustomizations": {
    "editorBracketMatch.background": "#FF1493",
    "editorBracketMatch.border": "#FF1493"
  },
  "git.confirmSync": false,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontFamily": "Iosevka, Menlo, Monaco, monospace",
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/dist/**/*": true,
    "**/node_modules/**/*": true,
    "**/__pycache__/": true,
    "**/*.py.bak": true,
    ".vscode/": true,
    "**/.tox/**": true,
    "**/coverage/**": true
  },
  "search.exclude": {
    "**/node_modules": true
  },
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "trailing-spaces.trimOnSave": true,
  "trailing-spaces.includeEmptyLines": true,
  "window.zoomLevel": 0,
  "typescript.check.npmIsInstalled": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": false,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.hideCursorInOverviewRuler": true,
  "search.location": "sidebar",
  "eslint.packageManager": "yarn",
  "editor.formatOnSave": true,
  "[python]": {
    "editor.tabSize": 4
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontWeight": "400"
}
