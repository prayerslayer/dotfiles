{
  "editor.fontFamily": "Iosevka, Menlo, Monaco, monospace",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.control",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.fontSize": 16,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "comments": false,
    "other": false,
    "strings": true
  },
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.folding": false,
  "editor.tabCompletion": "on",
  "editor.cursorBlinking": "solid",
  "editor.fontLigatures": true,
  "editor.autoIndent": true,
  "editor.lineNumbers": "interval",
  "editor.rulers": [
    80,
    120
  ],
  "zenMode.centerLayout": false,
  "workbench.fontAliasing": "auto",
  "workbench.colorCustomizations": {
    "editorBracketMatch.background": "#FF1493",
    "editorBracketMatch.border": "#FF1493"
  },
  "git.confirmSync": false,
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "terminal.integrated.fontFamily": "Iosevka, Menlo, Monaco, monospace",
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/dist/**/*": true,
    "**/node_modules/**/*": true,
    "**/__pycache__/": true,
    "**/*.py.bak": true,
    ".vscode/": true,
    "**/.tox/**": true,
    "**/coverage/**": true
  },
  "search.exclude": {
    "**/node_modules": true
  },
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "trailing-spaces.trimOnSave": true,
  "trailing-spaces.includeEmptyLines": true,
  "window.zoomLevel": 0,
  "typescript.check.npmIsInstalled": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": false,
  "python.formatting.provider": "autopep8",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.hideCursorInOverviewRuler": true,
  "search.location": "sidebar",
  "eslint.packageManager": "yarn",
  "editor.formatOnSave": true,
  "[python]": {
    "editor.tabSize": 4
  }
}
